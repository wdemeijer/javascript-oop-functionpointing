	<script src="jquery-1.7.1.js"></script>
	<script src="js/jquery.ba-bbq.js"></script>
	<script> 
		function anObject() {
		}
		anObject.prototype.constructor = anObject;
		anObject.prototype = {
			init: function() {
				this.callMe();
			},
			callMe: function() {
				var self = this;
				self.callYouBack(self, self.callMe2);
			},
			callYouBack: function(self, function_name, function_name2) {
				this.print123 = "print this.";
				alert("ghalo");
				var self = this;
				alert("ghalo4");
			},
			callMe2: function(self) {
				
				alert(self.print123);
			},
			callMe3: function(function_name) {
				alert("callMe3");
				function_name();
			},
			callMe4: function() {
				alert(this.print123);
			}
		}
		
		var thisObject = new anObject();
		thisObject.init();
	</script>
